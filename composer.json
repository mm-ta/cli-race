{
  "name": "mm-ta/grasshopper",
  "description": "A Cli Race Game",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammad Mahdi Taheri",
      "email": "mmtaheri.dev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-readline": "*",
    "wp-cli/php-cli-tools": "^0.11.22"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.42",
    "phpunit/phpunit": "^10.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  }
}
